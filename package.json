{
  "name": "storefront",
  "version": "0.0.0",
  "private": true,
  "author": "Riqra <support@riqra.com>",
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:server": "swc src/server -d .dist",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf -rf .server .next",
    "dev": "nodemon -q --watch src/server",
    "start": "node -r dotenv/config .dist/server",
    "boot": "run-s clean build start",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.{js,jsx,json,ts,tsx,html,css,md}\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "test:static": "run-p check-format lint typecheck",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "heroku-cleanup": "npm prune --production && rm -Rf .next/cache && find node_modules -depth -maxdepth 3 -type f -name '*swc*' -exec rm -rf {} \\; -prune && find node_modules -depth -maxdepth 1 -type d -name '*esbuild*' -exec rm -rf {} \\; -prune"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ],
    "*.{json,html,css,md}": [
      "prettier --write"
    ]
  },
